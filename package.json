{
  "name": "react-helloworld",
  "version": "1.0.0",
  "description": "react helloWorld",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "build": "webpack --config webpack.config.js",
    "serve": "webpack-dev-server --progress --inline --profile --colors --display-error-details --history-api-fallback true --open",
    "start": "npm run clean && npm run serve"
  },
  "author": "Haziz Khan",
  "authorGithub": "https://github.com/hazizkhan",
  "license": "ISC",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/react": "0.14.41",
    "@types/react-dom": "0.14.18",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "json-loader": "0.5.4",
    "raw-loader": "0.5.1",
    "sass-loader": "4.0.2",
    "source-map-loader": "0.1.5",
    "style-loader": "^0.18.2",
    "ts-loader": "0.9.5",
    "typescript": "2.2.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  }
}
